	<cffunction name="getReminderID" access="public" returntype="String" output="No">
  		<cfreturn variables.instance.ReminderID>
	</cffunction>
	 
	<cffunction name="setReminderID" access="public" returntype="boolean" output="No">
 		
		<cfargument name="ReminderID" required="Yes" type="String">
		
		<cfif ReminderID NEQ "NULL">	
			
			<cfif arguments.ReminderID EQ "@@" OR arguments.ReminderID EQ "">
				
				<cfset arguments.ReminderID = "NULL">
						
			<cfelseif NOT REQUEST.isintDataType(ReminderID)>
				<cf_gcGeneralErrorTemplate				
					message="<strong>ReminderID</strong> (#ReminderID#) is not of type <strong>int</strong>.">
			</cfif>
						
		</cfif>	
			
